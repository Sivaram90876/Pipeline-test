pipeline{
 
  agent any

  stages{

      stage("build"){
       
       steps{
        echo 'building the application...'
       }
      }
        stage("test"){
       
       steps{
        echo 'auto triggering...'
        echo 'testing the application...'
       }
      }
        stage("deploy"){
       
       steps{
        echo 'deploying the application...'
       }
      }
  }

}
